{
  "name": "waba-bikemap",
  "version": "1.0.0",
  "description": "Map of bike infrastructure in the Washington D.C. metro area for the Washington Area Bicyclists Association.",
  "scripts": {
    "start": "serve",
    "postinstall": "node nightwatch.conf.js",
    "test": "npm run test:unit && npm run test:e2e",
    "buffers": "node --max-old-space-size=32768 scripts/generate-buffers.js",
    "selenium-server-start": "selenium-standalone install && selenium-standalone start",
    "fetch": "node scripts/fetch.js",
    "test:unit": "node_modules/.bin/nightwatch --config nightwatch.conf.js --group unit",
    "test:e2e": "node_modules/.bin/nightwatch --config nightwatch.conf.js --group e2e"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcfemtech/hackforgood-waba-map.git"
  },
  "keywords": [
    "bike",
    "map",
    "bike",
    "lanes",
    "bike",
    "infrastructure",
    "WABA",
    "DC",
    "Washington",
    "DC"
  ],
  "author": "Code for DC",
  "bugs": {
    "url": "https://github.com/dcfemtech/hackforgood-waba-map/issues"
  },
  "homepage": "https://github.com/dcfemtech/hackforgood-waba-map#readme",
  "devDependencies": {
    "d3-queue": "^3.0.2",
    "eslint": "^3.3.0",
    "nightwatch": "^0.9.12",
    "request": "^2.74.0",
    "selenium-download": "^2.0.9",
    "selenium-standalone": "^6.0.1",
    "serve": "^1.4.0",
    "turf": "^3.0.14"
  }
}
